{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "palette": [
        {
          "name": "Pecan Brown",
          "slug": "primary",
          "color": "#B9905F"
        },
        {
          "name": "Eggshell White",
          "slug": "white",
          "color": "#FAFAF9"
        },
        {
          "name": "Tobacco Brown",
          "slug": "secondary",
          "color": "#504027"
        },
        {
          "name": "Dark",
          "slug": "dark",
          "color": "#1F2937"
        },
        {
          "name": "Obsidian",
          "slug": "obsidian",
          "color": "#262425"
        },
        {
          "name": "Light",
          "slug": "light",
          "color": "#000000"
        }
      ]
    },
    "layout": {
      "contentSize": "1024px",
      "wideSize": "1280px"
    },
    "typography": {
      "fontSizes": [
        {
          "name": "Small",
          "slug": "small",
          "size": "0.875rem"
        },
        {
          "slug": "regular",
          "name": "Regular",
          "size": "1.5rem"
        },
        {
          "slug": "large",
          "name": "Large",
          "size": "1.125rem"
        },
        {
          "slug": "xl",
          "name": "XL",
          "size": "1.25rem"
        },
        {
          "slug": "xxl",
          "name": "2XL",
          "size": "1.5rem"
        },
        {
          "slug": "xxxl",
          "name": "3XL",
          "size": "1.875rem"
        }
      ]
    }
  }
}